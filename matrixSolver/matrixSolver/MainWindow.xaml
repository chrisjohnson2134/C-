<Window x:Class="matrixSolver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:matrixSolver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle HorizontalAlignment="Left" Height="290" Margin="10,54,0,0" Stroke="Black" VerticalAlignment="Top" Width="390"/>
        <Label Content="Matrix Solver" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="150" FontSize="22"/>
        <ProgressBar x:Name="singleBar" Margin="110,79,442,325"/>
        <ProgressBar x:Name="multiBar" Margin="110,131,442,273"/>
        <Label Content="Single Core" HorizontalAlignment="Left" Margin="14,83,0,0" VerticalAlignment="Top"/>
        <Label Content="Multi Core" HorizontalAlignment="Left" Margin="14,133,0,0" VerticalAlignment="Top"/>
        <Button x:Name="computeButton" Content="Button" HorizontalAlignment="Left" Margin="539,54,0,0" VerticalAlignment="Top" Height="40" Width="118" Click="computeButton_Click"/>
    </Grid>
</Window>
